<?xml version="1.0" encoding="utf-8"?>
<TableLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
 >
    <TableRow  >
        <TextView
            android:textColor="@color/white"
            android:textAlignment="center"
            android:background="@drawable/border"
            android:textStyle="bold"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/tv_sr"/>
        <TextView
            android:textColor="@color/white"
            android:textAlignment="center"
            android:background="@drawable/border"
            android:textStyle="bold"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/tv_date"/>
        <TextView
            android:textColor="@color/white"
            android:textAlignment="center"
            android:background="@drawable/border"
            android:textStyle="bold"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/tv_service_id"/>
        <TextView
            android:textColor="@color/white"
            android:textAlignment="center"
            android:background="@drawable/border"
            android:textStyle="bold"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/tv_service_type"/>
        <TextView
            android:textColor="@color/white"
            android:textAlignment="center"
            android:textStyle="bold"
            android:background="@drawable/border"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/tv_customer"/>
        <TextView
            android:textColor="@color/white"
            android:textAlignment="center"
            android:background="@drawable/border"
            android:textStyle="bold"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/tv_rating"/>
        <TextView
            android:textColor="@color/white"
            android:textAlignment="center"
            android:background="@drawable/border"
            android:textStyle="bold"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/tv_incentive_five_star"/>
        <TextView
            android:textColor="@color/UIBlue2"
            android:textAlignment="center"
            android:background="@drawable/border"
            android:textStyle="bold"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/tv_approval"/>
        <TextView
            android:textColor="@color/UIBlue2"
            android:textAlignment="center"
            android:background="@drawable/border"
            android:textStyle="bold"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/tv_aincetive_amt"/>
        <TextView
            android:textColor="@color/UIBlue2"
            android:textAlignment="center"
            android:background="@drawable/border"
            android:textStyle="bold"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/tv_uniform_approval"/>
        <TextView
            android:textColor="@color/UIBlue2"
            android:textAlignment="center"
            android:background="@drawable/border"
            android:textStyle="bold"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/tv_paid_status"/>

    </TableRow>
</TableLayout>